{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "pages/api/users/**/*.js": {
      "maxDuration": 30
    }
  }
}
{
  "source": "/feedback/:id",
  "destination": "/api/feedback/:id"
}
